<?xml version="1.0" ?>
<!-- Configuration of the Ant build system to generate a Jar file --> 
<project name="HelperAgent" default="CreateJar">
  <target name="CreateJar" description="Create the customized helper">
        <jar jarfile="lib\HelperAgent.jar" basedir="target/classes" manifest="META-INF/agent.mf" />
  </target>
</project>

